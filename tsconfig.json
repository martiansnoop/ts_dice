{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "commonjs",
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "jsx": "react",
    "outDir": "tsc_out",
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "typings/main",
    "typings/main.d.ts"
  ],
  "filesGlob": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "typings/browser.d.ts"
  ]
}
